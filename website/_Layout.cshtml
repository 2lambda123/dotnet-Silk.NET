<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="Your one-stop-shop for high performance .NET graphics &amp; compute. A .NET Foundation project.">
  @{
      var title = Model.GetString("Title");
  }
  @if (title is null)
  {
      <title>
        Silk.NET - High-Speed &amp; Advanced .NET Graphics &amp; Compute
      </title>
  }
  else
  {
      <title>
        @title - Silk.NET
      </title>
  }
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet" />
  <link href="theme/nucleo.css" rel="stylesheet" />
  <link href="theme/fontawesome.css" rel="stylesheet" />
  <link href="theme/argon.css" rel="stylesheet" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/docsearch.js/2/docsearch.min.css" />
  <link rel="stylesheet" href="theme/docs.css" />
  <link rel="stylesheet" href="theme/silk.css" />
</head>

<body>
  @Html.RenderPartial("_theme/src/10-nav.cshtml")
  @Html.RenderPartial("_theme/src/20-contentroot.cshtml")
  <script src="@Url.Content("theme/jquery.min.js")" type="text/javascript"></script>
  <script src="@Url.Content("theme/popper.min.js")" type="text/javascript"></script>
  <script src="@Url.Content("theme/bootstrap.min.js")" type="text/javascript"></script>
  <script src="@Url.Content("theme/perfect-scrollbar.jquery.min.js")"></script>
  <script src="@Url.Content("theme/bootstrap-switch.js")"></script>
  <script src="@Url.Content("theme/nouislider.min.js")" type="text/javascript"></script>
  <script src="@Url.Content("theme/moment.min.js")"></script>
  <script src="@Url.Content("theme/datetimepicker.js")" type="text/javascript"></script>
  <script src="@Url.Content("theme/bootstrap-datepicker.min.js")"></script>
  <script async defer src="https://buttons.github.io/buttons.js"></script>
  <script src="@Url.Content("theme/argon.js")" type="text/javascript"></script>
  <script src="@Url.Content("theme/jquery.sharrre.js")"></script>
  <script src="@Url.Content("theme/docs.min.js")"></script>
  <script src="@Url.Content("theme/anchor.min.js")"></script>
  <script src="@Url.Content("theme/clipboard.min.js")"></script>
  <script src="@Url.Content("theme/holder.min.js")"></script>
  <script src="@Url.Content("theme/application.js")"></script>
  <script src="@Url.Content("theme/headroom.min.js")"></script>
  <script src="@Url.Content("theme/silk.js")"></script>
</body>

</html>