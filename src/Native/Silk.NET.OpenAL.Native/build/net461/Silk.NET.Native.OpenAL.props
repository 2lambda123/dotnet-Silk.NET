<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_Silk_NET_NativePackages_OpenAL_IsWindows Condition="'$([System.Runtime.InteropServices.RuntimeInformation]::IsOSPlatform($([System.Runtime.InteropServices.OSPlatform]::Windows)))' == 'true'">true</_Silk_NET_NativePackages_OpenAL_IsWindows>
    <_Silk_NET_NativePackages_OpenAL_IsMacOS Condition="'$([System.Runtime.InteropServices.RuntimeInformation]::IsOSPlatform($([System.Runtime.InteropServices.OSPlatform]::OSX)))' == 'true'">true</_Silk_NET_NativePackages_OpenAL_IsMacOS>
    <_Silk_NET_NativePackages_OpenAL_IsLinux Condition="'$([System.Runtime.InteropServices.RuntimeInformation]::IsOSPlatform($([System.Runtime.InteropServices.OSPlatform]::Linux)))' == 'true'">true</_Silk_NET_NativePackages_OpenAL_IsLinux>

    <_Silk_NET_NativePackages_OpenAL_NativeRuntime Condition=" '$(_Silk_NET_NativePackages_OpenAL_NativeRuntime)' == '' And '$(_Silk_NET_NativePackages_OpenAL_IsMacOS)' == 'true' And ('$(Prefer32Bit)' == 'false' Or '$(PlatformTarget)' == 'x64')">osx-x64</_Silk_NET_NativePackages_OpenAL_NativeRuntime>
    <_Silk_NET_NativePackages_OpenAL_NativeRuntime Condition=" '$(_Silk_NET_NativePackages_OpenAL_NativeRuntime)' == '' And '$(_Silk_NET_NativePackages_OpenAL_IsLinux)' == 'true' And ('$(Prefer32Bit)' == 'false' Or '$(PlatformTarget)' == 'x64')">linux-x64</_Silk_NET_NativePackages_OpenAL_NativeRuntime>
    <_Silk_NET_NativePackages_OpenAL_NativeRuntime Condition=" '$(_Silk_NET_NativePackages_OpenAL_NativeRuntime)' == '' And '$(_Silk_NET_NativePackages_OpenAL_IsWindows)' == 'true' And ('$(Prefer32Bit)' == 'true' Or '$(PlatformTarget)' == 'x86')">win-x86</_Silk_NET_NativePackages_OpenAL_NativeRuntime>
    <_Silk_NET_NativePackages_OpenAL_NativeRuntime Condition=" '$(_Silk_NET_NativePackages_OpenAL_NativeRuntime)' == '' And '$(_Silk_NET_NativePackages_OpenAL_IsWindows)' == 'true' And ('$(Prefer32Bit)' == 'false' Or '$(PlatformTarget)' == 'x64')">win-x64</_Silk_NET_NativePackages_OpenAL_NativeRuntime>

    <_Silk_NET_NativePackages_OpenAL_NativeLibName Condition="'$(_Silk_NET_NativePackages_OpenAL_NativeRuntime)' == 'win-x86'">soft_oal.dll</_Silk_NET_NativePackages_OpenAL_NativeLibName>
    <_Silk_NET_NativePackages_OpenAL_NativeLibName Condition="'$(_Silk_NET_NativePackages_OpenAL_NativeRuntime)' == 'win-x64'">soft_oal.dll</_Silk_NET_NativePackages_OpenAL_NativeLibName>
    <_Silk_NET_NativePackages_OpenAL_NativeLibName Condition="'$(_Silk_NET_NativePackages_OpenAL_NativeRuntime)' == 'osx-x64'">libopenal.dylib</_Silk_NET_NativePackages_OpenAL_NativeLibName>
    <_Silk_NET_NativePackages_OpenAL_NativeLibName Condition="'$(_Silk_NET_NativePackages_OpenAL_NativeRuntime)' == 'linux-x64'">libopenal.so</_Silk_NET_NativePackages_OpenAL_NativeLibName>
  </PropertyGroup>
  <ItemGroup>
    <Content Condition="'$(_Silk_NET_NativePackages_OpenAL_NativeRuntime)' != ''" Include="$(MSBuildThisFileDirectory)..\..\runtimes\$(_Silk_NET_NativePackages_OpenAL_NativeRuntime)\native\$(_Silk_NET_NativePackages_OpenAL_NativeLibName)">
      <Link>%(Filename)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <Visible>False</Visible>
    </Content>
  </ItemGroup>
</Project>
